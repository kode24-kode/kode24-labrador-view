{
    "contentMenus": {
        "default": {
            "inheritPath": "menu/content/basemenu.json",
            "items": {
                "style": {
                    "inheritPath": "menu/buttons/styles.json",
                    "group": "g4"
                },
                "expandedState": {
                    "icon": "labicon-arrows_horizontal",
                    "title": "Expand box",
                    "group": "g4",
                    "callback": "toggleKeyAndValue",
                    "key": "metadata.expandedState",
                    "onValue": true,
                    "offValue": false,
                    "bindings": [
                        {
                            "path": "metadata.expandedState",
                            "attribute": "value"
                        }
                    ]
                }
            }
        }
    }
}
