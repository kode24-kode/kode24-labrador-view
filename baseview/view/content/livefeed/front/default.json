{
    "boxTitle": "Live Feed",
    "boxDescription": "Displays a feed with live updates from a set of tags.",
    "boxKeywords": "live notice notices feed ticker studio breaking news event",
    "viewHelper": "external",
    "external": {
        "url": "{{{ get.current.filtered.url }}}{{{ get.current.filtered.query }}}",
        "type": "json",
        "fetchInPrepare": true
    },
    "viewportMap": {
        "fields": {
            "maxNoticesCount": "presets.vp_data",
            "visibleCount": {
                "viewport": {
                    "default": true
                }
            },
            "lineClamp": {
                "viewport": {
                    "default": true
                }
            }
        }
    }
}