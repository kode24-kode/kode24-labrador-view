{{ #get.current.filtered.adData.auId }}
<td class="cell" width="100%" valign="top">
    <table width="100%" style="padding-left: 10px; padding-right: 10px;" cellspacing="0" cellpadding="0" style="border: 0; padding-left: 10px; padding-right: 10px;">
        <tr>
            <td align="center" valign="top" {{ #get.current.filtered.isDebug }}style="border: 2px solid red; min-height: 2px;"{{ /get.current.filtered.isDebug }}>
                {{ #get.current.filtered.displayLabel }}<table width="100%" cellspacing="0" cellpadding="0" style="border: 0;">
                    <tr>
                        <td class="font-14 text-silver" align="left" valign="top" style="font-size: 14px; color: #555;">
                            {{ #get.current.filtered.isDebug }}
                            {{{ get.current.fields.format }}} (responsive)
                            {{ /get.current.filtered.isDebug }}
                            {{ ^get.current.filtered.isDebug }}
                            {{ get.current.filtered.label }}
                            {{ /get.current.filtered.isDebug }}
                        </td>
                    </tr>
                </table>{{ /get.current.filtered.displayLabel }}
                <table width="100%" cellspacing="0" cellpadding="0" style="border: 0;">
                    <tr>
                        <td align="left" valign="top">
                            <a href="https://delivery.adnuntius.com/c?auId={{{ get.current.filtered.adData.auId }}}&autor=true&iid=$(email as hash)" style="color: #000;">
                                <img src="https://delivery.adnuntius.com/i?auId={{{ get.current.filtered.adData.auId }}}{{ get.root.filtered.adnuntius.newsletter.targeting }}&format=image&iid=$(email as hash)" width="620" alt="{{ get.current.filtered.label }}" class="responsive-img" style="display: block; border: 0; max-width: 620px; width: 100%;" />
                            </a>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>
</td>
{{ /get.current.filtered.adData.auId }}