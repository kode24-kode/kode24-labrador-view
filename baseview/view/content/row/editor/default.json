                            "displayHotkeys": true,
                            "hotkeys": [{
                                "key": "D",
                                "controlkeys": ["shiftKey"]
                            }]
                        },
                        "fullwidth": {
                            "icon": "labicon-image_fullwidth",
                            "group": "sg1",
                            "title": "Options for full width display",
                            "text": "Full width options",
                            "bindings": [
                                {
                                    "path": "absoluteGridWidth",
                                    "attribute": "disabled",
                                    "valueTransformer": "hasValue",
                                    "valueTransformerOptions": {
                                        "value": 12,
                                        "negateBoolean": true
                                    }
                                }
