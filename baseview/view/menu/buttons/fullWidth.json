{
    "icon": "labicon-image_fullwidth",
    "text": "Full width",
    "title": "Full width display (Shift + F)",
    "callback": "toggleKeyAndValue",
    "key": "metadata.hasFullWidth",
    "onValue": true,
    "offValue": false,
    "bindings": [
        {
            "path": "metadata.hasFullWidth",
            "attribute": "value"
        }
    ],
    "displayHotkeys": true,
    "hotkeys": [
        {
            "key": "f",
            "controlkeys": ["shiftKey"]
        }
    ]
}