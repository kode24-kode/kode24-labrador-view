            "offset": null,
            
            "items": {

                "addRowBefore": {
                    "trigger": ["drop"],
                    "callback": "addRowBefore",
                    "group": "g3",
                    "text": "Add row",
                    "displayCondition": "structureIsFirstChild"
                },
                "addRowAfter": {
                    "trigger": ["drop"],
                    "callback": "addRowAfter",
                    "group": "g3",
                    "text": "Add row"
                }
            },
            "params": {}
        }

    }
